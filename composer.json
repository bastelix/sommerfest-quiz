{
    "name": "sommerfest/quiz",
    "description": "Slim-based quiz application using UIkit3",
    "type": "project",
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "twig/twig": "^3.8",
        "slim/twig-view": "^3.3",
        "setasign/fpdf": "^1.8",
        "endroid/qr-code": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.9"
    }
}
